                    </div>
                    <div>
                        <label class="bg-primary text-white h3 time-warn" id="secon2" style="width: 30px; border-radius: 4px; margin: 2px">0 </label>
                        <label class="bg-primary text-white h3 time-warn" id="secon1" style="width: 30px; border-radius: 4px; margin: 2px">0 </label>
                    </div>

                </div>
                <div class="row   m-4 body-content justify-content-center">
                        <button class=" btn btn-primary text-bold" id="btn-sendResult">Lưu bài</button>
                </div>
                
                    <div class="row " id="list-Question"  >

                    </div>
                
                <div class="row justify-content-center">
                    <button class=" m-auto btn btn-danger m-3" id="btn-finishTest">Kết thúc</button>
                </div>
            </div>
        </div>
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Style/Js/Test.js"></script>
</body>
</html>
